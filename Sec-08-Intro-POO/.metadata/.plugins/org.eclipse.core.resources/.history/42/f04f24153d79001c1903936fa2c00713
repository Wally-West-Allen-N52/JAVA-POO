	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner read = new Scanner(System.in);
		
		System.out.print("What is the dollar price? ");
		double dollar = read.nextDouble();
		
		System.out.println("");
	}

}
