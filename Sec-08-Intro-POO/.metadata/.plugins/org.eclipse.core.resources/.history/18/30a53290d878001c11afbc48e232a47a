package Entities;

public class FinalGrade {
	public String name;
	public double grade1;
	public double grade2;
	public double grade3;
	
	public double finalGrade() {
		return (grade1 + grade2 + grade3) / 2;
	}
	public double missingpoints() {
		if(finalGrade() < 60.0)  {
			return 60.0 - finalGrade();
		}
	}
}
